{
  "name": "next.js-starter",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build -d",
    "dev": "next dev --port 4001 --turbopack",
    "lint": "TIMING=1 eslint \"**/*.{ts,tsx,js,jsx}\" --fix",
    "next-lint": "next lint",
    "start": "next start --port 4001",
    "test": "vitest run",
    "lint-staged-files": "lint-staged --allow-empty",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "Chrome 118",
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "3.13.9",
    "@apollo/experimental-nextjs-app-support": "0.12.3",
    "@bprogress/next": "^3.2.12",
    "@dimjs/utils": "^2.1.2",
    "@heroui/react": "^2.8.2",
    "@hookform/resolvers": "^5.2.1",
    "@hyperse/hero-tel-input": "^1.0.1",
    "@hyperse/logger": "^1.1.7",
    "@hyperse/logger-plugin-console": "^1.0.11",
    "@hyperse/next-config": "^1.0.6",
    "@hyperse/next-inspector": "^1.1.9",
    "@iconify/react": "^6.0.0",
    "@next/bundle-analyzer": "15.4.7",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "ahooks": "^3.9.3",
    "apollo-upload-client": "18.0.1",
    "cookies-next": "6.1.0",
    "framer-motion": "^12.23.12",
    "gql.tada": "1.8.13",
    "graphql": "16.11.0",
    "jotai": "^2.13.1",
    "next": "15.4.7",
    "next-intl": "^4.3.4",
    "next-recaptcha-v3": "^1.5.2",
    "next-themes": "^0.4.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "sharp": "^0.34.3",
    "simplebar-react": "^3.3.2",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^2.0.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@0no-co/graphqlsp": "1.15.0",
    "@changesets/cli": "2.29.6",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@hyperse/eslint-config-hyperse": "^1.4.7",
    "@svgr/webpack": "^8.1.0",
    "@types/apollo-upload-client": "^18.0.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "commitizen": "4.3.1",
    "cross-env": "10.0.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "husky": "9.1.7",
    "is-ci": "4.1.0",
    "lint-staged": "16.1.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
